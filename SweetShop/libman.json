{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "jquery@3.5.1",
      "destination": "wwwroot/lib/jquery/dist/",
      "files": [
        "jquery.js",
        "jquery.min.js"
      ]
    },
    {
      "library": "toastr.js@2.1.4",
      "destination": "wwwroot/lib/toastr/",
      "files": [
        "toastr.min.js",
        "toastr.css",
        "toastr.min.css",
        "toastr.js.map"
      ]
    },
    {
      "provider": "cdnjs",
      "library": "globalize@1.7.0",
      "destination": "wwwroot/lib/globalize/"
    },
    {
      "provider": "cdnjs",
      "library": "cldrjs@0.5.5",
      "destination": "wwwroot/lib/cldrjs/"
    },
    {
      "provider": "jsdelivr",
      "library": "jquery-validation-globalize@0.1.1",
      "destination": "wwwroot/lib/jquery-validation-globalize/"
    }
  ]
}